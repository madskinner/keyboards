store(&TARGETS) 'any'
store(&KEYBOARDVERSION) '10.0' 
store(&mnemoniclayout) '0'

c NFC qwerty default for Mac, Linux and web, touch?
store(&NAME) 'sil-qwerty-nfc'
begin Unicode > use(main)

group(main)
c platform("web") + notany(nochars) > use(web)
platform("web") > use(web)
platform("touch") > use(touch)
c platform("hardware") > use(hardware) 
nomatch() > use(hardware) 

group(web) using keys
c a qwerty web 
+ [K_A] > "a web keyboard"     

group(touch) using keys 
c a qwerty touch
+ [K_A] > "a touch keyboard"

group(hardware)
platform("windows") > use(windows)
platform("linux")   > use(linux)
platform("macos")   > use(macos)   

group(linux) using keys
c can't detect baselayout
c so defaults to US?
+ [K_A] > "a linux keyboard"

group(macos) using keys
c can't detect baselayout
c so defaults to US?
+ [K_A] > "a macos keyboard"

group(windows)
c + any() > "a windows keyboard"
